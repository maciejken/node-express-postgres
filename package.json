{
  "name": "node-express-postgres-template",
  "version": "0.0.1",
  "description": "A starter project for Node application with express, postgres and sequelize",
  "main": "server.js",
  "keywords": [
    "Node.js",
    "Node",
    "express",
    "template",
    "seed",
    "postgresql",
    "postgres",
    "heroku",
    "sequelize",
    "passport",
    "winston",
    "express-validator"
  ],
  "homepage": "https://github.com/pamigomp/node-express-postgres-template",
  "bugs": {
    "url": "https://github.com/pamigomp/node-express-postgres-template/issues"
  },
  "author": "Michal Pietrzak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pamigomp/node-express-postgres-template.git"
  },
  "scripts": {
    "start": "npm run apidoc && node server.js",
    "dev": "npm run apidoc && nodemon server.js",
    "apidoc": "apidoc -i app/routes/ -o apidoc",
    "cluster": "node cluster.js",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js"
  },
  "dependencies": {
    "apidoc": "^0.17.6",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "connect-flash": "^0.1.1",
    "consign": "^0.1.6",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "helmet": "^3.9.0",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^7.4.0",
    "response-time": "^2.3.2",
    "sequelize": "^4.28.6",
    "serve-favicon": "^2.4.5",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-plugin-node": "^5.2.1",
    "expect.js": "^0.3.1",
    "grunt": "^1.0.1",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-less": "^1.4.1",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-jscs": "^3.0.1",
    "grunt-nodemon": "^0.4.2",
    "mocha": "^4.0.1",
    "nodemon": "^1.14.3",
    "sequelize-cli": "^3.2.0",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": "^8.9.1",
    "npm": "^5.6.0"
  },
  "private": false
}
